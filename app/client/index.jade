doctype html
html
    head
        title JS-Course
        script(src="framework.js")
        script(src="scripts.js")
        link(rel="stylesheet" href="styles.css")
    body
        div(sg-controller="tableCtrl")
            table(border="1" align="center" id="rpCtrl")
                tr
                    th State
                    th City
                    th Street
                tr(sg-repeat="item in data")
                    td(sg-bind="item.state")
                    td(sg-bind="item.city")
                    td(sg-bind="item.street")
            div(class="paginator" sg-repeat="pageNumber in pages")
                button(sg-click="Click(pageNumber)" sg-bind="pageNumber")
        script Compiler.$compileRoot();
